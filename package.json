{
  "name": "yap-sdk",
  "version": "1.0.3",
  "description": "YAP SDK for JavaScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "prepublish": "yarn build",
  "repository": "git@github.com:youngapp/yap-sdk.git",
  "author": "GÃ©rard TOKO <gerard@youngapp.co>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "unbuild": "rm -rf dist",
    "build": "yarn unbuild && ENV=dev tsc --declaration",
    "global": "yarn global add yarn nyc tslint typescript",
    "lint": "./node_modules/.bin/tslint -c tslint.json 'src/**/*.ts'",
    "test": "jest --verbose --env=jsdom --coverage --testResultsProcessor ./node_modules/jest-html-reporter"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/jest": "^24.0.15",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "jest-html-reporter": "^2.5.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "whatwg-fetch": "^3.0.0"
  }
}
